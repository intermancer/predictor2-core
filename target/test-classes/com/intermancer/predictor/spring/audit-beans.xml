<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="
       http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
 
 
	<!-- Audit beans -->
	<bean id="loggingInterceptor" class="com.intermancer.predictor.audit.LoggingInterceptor" />

	<bean id="beanNameAutoproxy" class="org.springframework.aop.framework.autoproxy.BeanNameAutoProxyCreator">
		<property name="beanNames" value="totalCountFCL" />
		<property name="interceptorNames" value="loggingInterceptor" />
	</bean>

</beans>

